{
    "window.zoomLevel": -1,
    "pandoc.pdfOptString": "--from markdown  --title=true --listings -V geometry:margin=1in",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "One Dark Pro",
    "editor.fontSize": 20,
    "glsllint.glslangValidatorPath": "/usr/bin/glslangValidator",
    "material-icon-theme.showUpdateMessage": false,
    "editor.fontLigatures": true,
    "latex-workshop.view.pdf.viewer": "external",
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "python.pythonPath": "/usr/bin/python3",
    "editor.wordWrap": "on",
    "editor.mouseWheelZoom": true,
    "terminal.external.linuxExec": "konsole",
    "jupyter.appendResults": false,
    "cSpell.language": "en,fr",
    "cSpell.userWords": [
        "bilat",
        "epfl",
        "kaggle",
        "lâ€™epfl"
    ],
    "breadcrumbs.enabled": true,
    "latex-workshop.chktex.enabled": true,
    "latex-workshop.intellisense.package.enabled": true,
    "latex-workshop.intellisense.surroundCommand.enabled": true,
    "latex-workshop.intellisense.unimathsymbols.enabled": true,
    "latex-workshop.latex.clean.enabled": true,
    "latex-workshop.view.pdf.ref.viewer": "external",
    "latex-workshop.latex.autoClean.run": "onBuilt",
    "java.configuration.checkProjectSettingsExclusions": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.jediEnabled": false,
    "python.linting.pylintEnabled": true
}